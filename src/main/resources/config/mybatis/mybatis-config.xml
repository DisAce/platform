<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<plugins>
		<!--
		指定数据库分页方言Dialect, 其它方言:OracleDialect,SQLServerDialect,SybaseDialect,DB2Dialect,
		PostgreSQLDialect,MySQLDialect,DerbyDialect,HSQLDialect
		-->
		<plugin interceptor="org.coiol.platform.common.mybatis.OffsetLimitInterceptor">
			<property name="dialectClass" value="org.coiol.platform.common.mybatis.dialect.MySQLDialect"/>
		</plugin>
	</plugins>
	<mappers>
		<mapper resource="sqlmap/common_SqlMap.xml" />
		<mapper resource="sqlmap/BaseModulesMapper.xml" />
		<mapper resource="sqlmap/BaseLoginLogMapper.xml" />
		<mapper resource="sqlmap/BaseRoleModuleMapper.xml" />
		<mapper resource="sqlmap/BaseRolesMapper.xml" />
		<mapper resource="sqlmap/BaseUserRoleMapper.xml" />
		<mapper resource="sqlmap/BaseUsersMapper.xml" />
		<mapper resource="sqlmap/BaseFieldsMapper.xml" />
		<mapper resource="sqlmap/BaseSupplierMapper.xml" />
		<mapper resource="sqlmap/BaseSupplierPriceMapper.xml" />
		<mapper resource="sqlmap/BaseMaterialMapper.xml" />
		<mapper resource="sqlmap/BaseShipmentMapper.xml" />
	</mappers>
	
</configuration>